{
    "name": "pastock/pawork",
    "description": "Working day finder for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.1 | ^8.0"
    },
    "require-dev": {
        "illuminate/console": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/container": "^5.6 | ^6 | ^7 | ^8",
        "illuminate/events": "^5.6 | ^6 | ^7 | ^8",
        "mileschou/codegener": "^1.4",
        "phpunit/phpunit": "^7.5 | ^8.5 | ^9.3",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Pastock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app",
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
